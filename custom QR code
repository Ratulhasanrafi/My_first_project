import qrcode
print("create your custom QRcode")

data = input("enter your data: ")

qr = qrcode.QRCode(
    version=2,
    error_correction=qrcode.constants.ERROR_CORRECT_L,
    box_size=10,
    border=4,
)

qr.add_data(data)
qr.make(fit=True)

img = qr.make_image(fill_color=input("enter fill color :"), back_color=input("background colour :"))

img.save("qrcode.png")